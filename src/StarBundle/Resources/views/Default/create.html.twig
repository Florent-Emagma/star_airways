{% extends 'base.html.twig' %}
{% block body %}
    <div class="container">
        <div class="row">
            {{ form_start(form) }}
            {{ form_row(form.depart) }}
            {{ form_row(form.arrivee) }}
            {{ form_row(form.horaire) }}
            {{ form_row(form.place) }}
            {{ form_row(form.prix) }}
            {{ form_end(form) }}
        </div>
        <div class="row">
            {{ form_errors(form) }}
        </div>
    </div>
{% endblock %}